{
    "name": "renoki-co/laravel-firebase-analytics",
    "description": "Laravel Firebase Analytics adds blade directives to initialize, log events and set user properties for Firebase Analytics.",
    "keywords": ["laravel", "php", "google", "firebase", "analytics", "blade", "directives"],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-co/laravel-firebase-analytics",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\FirebaseAnalytics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\FirebaseAnalytics\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.0.4",
        "orchestra/testbench": "^4.0|^5.0|^6.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\FirebaseAnalytics\\FirebaseAnalyticsServiceProvider"
            ]
        }
    }
}
